  "description": "",
  "main": "index.js",
  "types": "./types/global.d.ts",
  "scripts": {
    "start": " nodemon index.js",
    "build": "npm run lint && tsc -p .",
    "start:local": "NODE_ENV=local nodemon index.js",
    "start:test": "NODE_ENV=test node build/src/server.js",
    "start:production": "NODE_ENV=production pm2-runtime start build/src/server.js -i 2",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "test:custom": "npm test -- tests/unit-tests/lib/crypto.spec.ts",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint ",
    "test": "npx jest --coverage --silent -b",
    "test:ci": "jest --runInBand --silent -b",
    "test:watch": "npx jest --watch",
    "test:log": "npx jest --coverage  -b"
  },
